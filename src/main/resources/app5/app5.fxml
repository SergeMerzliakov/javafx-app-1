<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="150.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.epistatic.app5.controller.ApplicationController">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <Button id="exitButton" fx:id="exitButton" cancelButton="true" defaultButton="true" minWidth="80.0" mnemonicParsing="false" onAction="#closeApplication" text="Exit" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />
            <Label layoutX="21.0" layoutY="14.0" minHeight="32.0" text="Name" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="10.0" />
            <Label fx:id="nameLabel" layoutX="76.0" layoutY="10.0" maxHeight="26.0" minHeight="32.0" styleClass="sunken-label" text="Jane Smith" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="95.0" AnchorPane.topAnchor="10.0" />
            <Button layoutX="327.0" layoutY="13.0" mnemonicParsing="false" onAction="#rename" text="Rename" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
         </children>
      </AnchorPane>
   </children>
    <stylesheets>
        <URL value="@../bootstrap3.css" />
        <URL value="@app5.css" />
    </stylesheets>
</VBox>
